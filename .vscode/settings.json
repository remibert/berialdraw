{
	"cmake.sourceDirectory": "${workspaceFolder}/berialdraw",
	"cmake.debugConfig": {
		"cwd": "${workspaceFolder}/berialdraw"
	},
	"cmake.buildDirectory": "${env:HOME}/Downloads/build",
	"files.associations": {
		"ios": "cpp",
		"__config": "cpp",
		"tuple": "cpp",
		"__availability": "cpp",
		"deque": "cpp",
		"ostream": "cpp",
		"print": "cpp",
		"streambuf": "cpp",
		"string": "cpp",
		"cstdio": "cpp",
		"__assertion_handler": "cpp",
		"locale": "cpp",
		"typeinfo": "cpp",
		"version": "cpp",
		"__hash_table": "cpp",
		"__node_handle": "cpp",
		"bitset": "cpp",
		"format": "cpp",
		"fstream": "cpp",
		"complex": "cpp",
		"bit": "cpp",
		"compare": "cpp",
		"random": "cpp",
		"functional": "cpp",
		"istream": "cpp",
		"limits": "cpp",
		"mutex": "cpp",
		"sstream": "cpp",
		"stop_token": "cpp",
		"variant": "cpp"
	},
	"cmake.configureSettings": {
		"SDL3_INCLUDE_DIRS": "/Library/Frameworks/SDL3.framework"
	},
	"editor.minimap.enabled": false,
	"editor.renderWhitespace":"all",
	"editor.suggest.insertMode": "replace",
	"workbench.editor.enablePreview": false,
	"workbench.tree.indent": 10,
	"workbench.tree.renderIndentGuides": "always",
	"workbench.colorTheme": "Visual Studio Light",
	"workbench.iconTheme": "vscode-icons",
	"extensions.ignoreRecommendations": true,
	"editor.autoClosingBrackets": "never",
	"editor.autoClosingQuotes": "never",
	"editor.quickSuggestions": {
		"other": "off"
	},
	"editor.snippetSuggestions": "none"
}